//Á≠æÂà∞
    async  SignTask(user) {
        try {
            DoubleLog(`üïäË¥¶Âè∑[${user.index}] ÂºÄÂßãÁ≠æÂà∞...`);
            let urlObject = {
                method: 'post',
                url: `https://webapi.qmai.cn/web/cmk-center/sign/takePartInSign`,
                headers: this.headers,
                data:{
                    "activityId": "983701274523176960",
                    "appid": "wx3423ef0c7b7f19af"
                  }
            }
            //
            let { data: result} = await axios.request(urlObject)
            //console.log(result);
            if (result?.status == true) {
                //ÊâìÂç∞Á≠æÂà∞ÁªìÊûú
                DoubleLog(`üïäË¥¶Âè∑[${user.index}] Á≠æÂà∞ÊàêÂäüüéâ`);
            }if(result?.data.sign == "false") {
                DoubleLog(`üïäË¥¶Âè∑[${user.index}] Á≠æÂà∞Â§±Ë¥•:ÂéüÂõ†Êú™Áü•üö´`)
            }if (result?.status == false) {
                DoubleLog(`üïäË¥¶Âè∑[${user.index}] Á≠æÂà∞Â§±Ë¥•:${result.message}üö´`)
            }
            
            
        } catch (e) {
           console.log(e);
        }
    }
//ÁßØÂàÜ
async  account(user) {
    try {
        DoubleLog(`üïäË¥¶Âè∑[${user.index}] ÂºÄÂßãÊü•ËØ¢ÁßØÂàÜ...`);
        let urlObject = {
            method: 'post',
            url: `https://webapi.qmai.cn/web/mall-apiserver/integral/user/points-info`,
            headers: this.headers,
            data: {
                "appid": "wx3423ef0c7b7f19af"
            }
        }
        //
        let { data: result} = await axios.request(urlObject)
        //console.log(result);
        if (result?.status == true) {
            //ÊâìÂç∞Á≠æÂà∞ÁªìÊûú
            DoubleLog(`üïäË¥¶Âè∑[${user.index}] ÂΩìÂâçÁßØÂàÜ[${result.data.totalPoints}]üéâ`);
        }else {
            DoubleLog(`üïäË¥¶Âè∑[${user.index}] ÁßØÂàÜÊü•ËØ¢Â§±Ë¥•:${result.message}üö´`)
        }
        
        
    } catch (e) {
        console.log(e);
    }
}
