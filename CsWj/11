// ============================================================================================================
const $ = new Env('vxÁà∑Áà∑‰∏çÊ≥°Ëå∂') 
const axios = require('axios')
const env_name = 'yybpc' //ÁéØÂ¢ÉÂèòÈáèÂêçÂ≠ó
const env = process.env[env_name] || '' 
const Notify = 1
const debug = 0
let scriptVersionNow = "1.0.0";
let msg = "";

//==================================ËÑöÊú¨ÂÖ•Âè£ÂáΩÊï∞main()==============================================================

async function main() {
    if (env == '') {
        //Ê≤°ÊúâËÆæÁΩÆÂèòÈáè,Áõ¥Êé•ÈÄÄÂá∫
        console.log(`Ê≤°ÊúâÂ°´ÂÜôÂèòÈáè,ËØ∑Êü•ÁúãËÑöÊú¨ËØ¥Êòé: ${env_name}`)
        return
    }
    let user_ck = env.split('\n')
    DoubleLog(`\n========= ÂÖ±ÊâæÂà∞ ${user_ck.length} ‰∏™Ë¥¶Âè∑ =========`);
    let index = 1 //Áî®Êù•ÁªôË¥¶Âè∑Ê†áËÆ∞Â∫èÂè∑, ‰ªé1ÂºÄÂßã
    for (let ck of user_ck) {
        if (!ck) continue //Ë∑≥ËøáÁ©∫Ë°å
        let ck_info = ck.split('&')
        let token = ck_info[0] 
        //let uid = ck_info[0]
        //let deviceCode = ck_info[2]
        let user = {
            index: index,
            token, 
            //uid,
            //deviceCode,
        }
        index = index + 1 //ÊØèÊ¨°Áî®ÂÆåÂ∫èÂè∑+1
        //ÂºÄÂßãË¥¶Âè∑‰ªªÂä°
        let Run = new run();
        await Run.userTask(user)
        //ÊØè‰∏™Ë¥¶Âè∑‰πãÈó¥Á≠â1~5ÁßíÈöèÊú∫Êó∂Èó¥
        let rnd_time = Math.floor(Math.random() * 4000) + 1000
        console.log(`ÈöèÊú∫Á≠âÂæÖ${rnd_time / 1000}Áßí...`)
        await $.wait(rnd_time)
    }
}
// ======================================ÂºÄÂßã‰ªªÂä°=========================================
class run {
    constructor(user) {
        
    }
async  userTask(user) {
    console.log(`\n========= Ë¥¶Âè∑[${user.index}]ÂºÄÂßã‰ªªÂä° =========`)
    this.headers= {
        "qm-from": "wechat",
        "qm-user-token": user.token,
        'User-Agent': 'Mozilla/5.0 (iPhone; CPU iPhone OS 16_6 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Mobile/15E148 MicroMessenger/8.0.53(0x18003531) NetType/WIFI Language/zh_CN',
    }
    await this.SignTask(user)
    await wait(2)
    await this.account(user)
    await this.userSignStatistics(user)
}
