async userSignStatistics(user) {
    try {
        DoubleLog(`üïäË¥¶Âè∑[${user.index}] ÂºÄÂßãÊü•ËØ¢ËøûÁª≠Á≠æÂà∞ÊÉÖÂÜµ...`);
        let urlObject = {
            method: 'post',
            url: `https://webapi.qmai.cn/web/cmk-center/sign/userSignStatistics`,
            headers: this.headers,
            data: {
                "activityId": "983701274523176960",
                "appid": "wx3423ef0c7b7f19af"
            }
        }
        let { data: result } = await axios.request(urlObject)
        // console.log('userSignStatistics ', result);
        if (result?.status == true) {
            //ÊâìÂç∞Á≠æÂà∞ÁªìÊûú
            DoubleLog(`üïäË¥¶Âè∑[${user.index}] ÂΩìÂâçËøûÁª≠Á≠æÂà∞[${result.data.signDays}]Â§©Ôºå‰∏ã‰∏ÄÈò∂ÊÆµÂ•ñÂìÅ[${result.data.nextRewardList[0].rewardList[0].rewardName}]ÔºåËøòÈúÄË¶ÅÁ≠æÂà∞[${result.data.nextSignDays}]Â§©üéâ`);
        } else {
            DoubleLog(`üïäË¥¶Âè∑[${user.index}] ËøûÁª≠Á≠æÂà∞Êü•ËØ¢Â§±Ë¥•:${result.message}üö´`)
        }
    } catch (e) {
        console.log(e);
    }
}
